!XXX methodsFor: 'as yet unclassified' stamp: 'WAL 4/7/2023 19:03:17' prior: 50609560!
extractVowels

	| string vowels |
	
	string := 'abcdefguijp'.
	vowels := 'aeiou'.

	^  string select: [ : element |  (vowels occurrencesOf: element > 0 ) 	]
	
			"se observa que los strings son colecciones"
	

	
	! !


	| string vowels |
	
	string := 'abcdefguijp'.
	vowels := 'aeiou'.

	^  string select: [ : element |  (vowels occurrencesOf: element > 0 ) 	]
	
			"se observa que los strings son colecciones"!
