cars
	"| allCars ix |
	
	allCars := Set new.
	ix := 1.
	[ix <= sectors size] whileTrue: [ | s |
		s := sectors at: ix.
		allCars addAll: s cars.
		ix := ix + 1.
	]."

	^ sectors inject: Set new into: [ :allCars :sector | 	allCars addAll: sector cars ; yourself ]




	"^ allCars"
	! !

