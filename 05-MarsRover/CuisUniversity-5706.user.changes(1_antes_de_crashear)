test09LeftCommandTurnsLeft

	| marsRover |

	marsRover := MarsRover startOn: 1@1 facing: #Norte.
	
	marsRover giveCommands: 'l'.
	
	self assert: 1@1       equals: marsRover position.
	self assert: #Oeste equals: marsRover pointing.
	
	! !

