giveCommands: aCommandSequence

	aCommandSequence do: [ : command |
		command = $f ifTrue: [ | x y |
			x := position x.
			y := position y.
			position := x@(y+1).
		].
		command = $b ifTrue: [ | x y |
			x := position x.
			y := position y.
			position := x@(y-1).
		]
	].

	aCommandSequence = 'l' ifTrue: [
		pointing := #Oeste
	].
	aCommandSequence = 'll' ifTrue: [
		pointing := #Sur
	].
	aCommandSequence = 'lll' ifTrue: [
		pointing := #East	
	].

