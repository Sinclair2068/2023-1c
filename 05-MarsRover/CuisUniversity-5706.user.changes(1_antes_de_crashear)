!MarsRover methodsFor: 'control' stamp: 'WAL 5/12/2023 22:26:36' prior: 50646295!
giveCommands: aCommandSequenceString

	aCommandSequenceString do: [ : command |
		command = $f ifTrue: [ | x y |
			x := position x.
			y := position y.
			position := x@(y+1).
		]
	].! !

