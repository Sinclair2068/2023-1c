!Track methodsFor: 'accessing' stamp: 'WAL 5/7/2023 00:17:08' prior: 50640967!
cars
	| allCars ix |
	
	allCars := Set new.
	ix := 1.
	[ix <= sectors size] whileTrue: [ | s |
		s := sectors at: ix.
		allCars addAll: s cars.
		ix := ix + 1.
	].

	"sectors inject: Set new into: [ :
		
		
		
		]

"


	^ allCars
	! !

