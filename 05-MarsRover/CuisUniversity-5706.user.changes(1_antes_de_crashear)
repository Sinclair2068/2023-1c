test06TwoBackwardCommandMovesTwoBackward

	| marsRover |

	marsRover := MarsRover startOn: 1@1 facing: #Norte.
	
	marsRover giveCommands: 'b'.
	
	self assert: 1@-1       equals: marsRover position.
	self assert: #Norte equals: marsRover pointing.
	
	! !
