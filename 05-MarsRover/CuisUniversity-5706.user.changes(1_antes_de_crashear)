!MarsRover methodsFor: 'control' stamp: 'WAL 5/12/2023 22:44:42' prior: 50646825!
giveCommands: aCommandSequence

	aCommandSequence do: [ : command |
		command = $f ifTrue: [ | x y |
			x := position x.
			y := position y.
			position := x@(y+1).
		].
		command = $b ifTrue: [ | x y |
			x := position x.
			y := position y.
			position := x@(y-1).
		]
	].

	aCommandSequence = 'l' ifTrue: [
		
		pointing := #Oeste
		
	]

! !

