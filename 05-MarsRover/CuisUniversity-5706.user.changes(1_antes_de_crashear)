!MarsRover methodsFor: 'control' stamp: 'WAL 5/13/2023 12:39:46' prior: 50648513!
giveCommands: aCommandSequence

	aCommandSequence do: [ : command |
		command = $f ifTrue: [
			position := position + (0@1).
		].
		command = $b ifTrue: [
			position := position + (0@-1).
		].
		command = $l ifTrue: [ 
			pointing := pointing left.
		].
	].

	aCommandSequence = 'r' isTrue: [
		pointing := East new.
	].
! !

