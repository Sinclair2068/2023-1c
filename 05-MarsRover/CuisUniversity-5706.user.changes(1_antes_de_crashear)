!GrandPrix methodsFor: 'simulation' stamp: 'WAL 5/7/2023 00:08:10' prior: 50640681!
advance: aTimeLapse
	
	| cars |
	
	cars := track cars asOrderedCollection.
	"ix := 1.
	[ix <= cars size] whileTrue: [ | car |
		car := cars at: ix.
		self relocate: car after: aTimeLapse.
		ix := ix + 1.
	]."

	cars do: [ : car |
		
		self relocate: car after: aTimeLapse.
		
		]

	
! !

