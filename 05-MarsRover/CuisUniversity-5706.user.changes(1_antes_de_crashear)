	^self subclassResponsibility ! !

	zero := Entero with: 0.!

	one := Entero with: 1.!

	two := Entero with: 2.!

	three:= Entero with: 3.!

	four := Entero with: 4.!

	five := Entero with: 5.!

	eight := Entero with: 8.!

	negativeOne := Entero with: -1.!

	negativeTwo := Entero with: -2.!

one!

two!

one / two!

	oneFifth := one / five.!

