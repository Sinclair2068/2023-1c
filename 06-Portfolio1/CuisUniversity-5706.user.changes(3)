giveCommands: aCommandSequence

	aCommandSequence do: [ : commandCharacter |
		
		"| command |
		
		command := Command for: commandCharacter.
		
		command executeOn: self."
		
		| command |
		
		command := commands detect: [ : each | each class handles: commandCharacter ]  ifNone: [].
		
		command ifNotNil: [
			command executeOn: self.
		]
	].! !

