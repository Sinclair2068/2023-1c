!Track methodsFor: 'accessing' stamp: 'AS 5/3/2023 15:01:13' prior: 50657786!
cars
	| allCars |
	
	allCars := Set new.
	sectors do: [ :sector | allCars addAll: sector cars ].
	^ allCars
	! !

