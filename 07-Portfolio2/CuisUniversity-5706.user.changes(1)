!Crew methodsFor: 'as yet unclassified' stamp: 'AS 4/26/2023 14:39:24' prior: 50608053!
buryTreasureAt: aPosition

	| terrain |
	
	terrain := island terrainAt: aPosition.
	
	pirates do: [:pirate |
	
		pirate buryTreasureAt: aPosition in: terrain.
	].
	! !

