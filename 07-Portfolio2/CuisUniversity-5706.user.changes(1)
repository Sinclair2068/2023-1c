!City methodsFor: 'as yet unclassified' stamp: 'AS 4/27/2023 11:33:27' prior: 50620810!
energyConsumption
	
	| index total |
	
	index := 1.
	total := 0.
	
	[index <= zones size] whileTrue: [ | zone |
		zone := zones at: index.
		total := total + (zone energyConsumption * zone getNumberOfCells).
		index := index + 1.
	].

	^ total.! !

