!classDefinition: #CoffeeShopTest category: 'IngSof1-2019-2C-1Parcial' stamp: 'AS 5/3/2023 14:36:26'!
TestCase subclass: #CoffeeShopTest
	instanceVariableNames: 'billGates adaLovelace alanKay salesSystem'
	classVariableNames: ''
	poolDictionaries: ''
