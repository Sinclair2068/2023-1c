!Map methodsFor: 'as yet unclassified' stamp: 'AS 4/26/2023 15:35:24' prior: 50606684!
lastPosition

	| acc ix |

	^steps inject: startPosition into: [:position | position + 1].
	
	"
	acc := startPosition.
	ix := 1.
	
	[ix <= steps size] whileTrue: [
		acc := acc + (steps at: ix)	.
		ix := ix + 1.
	].

	^ acc
	"
	! !

