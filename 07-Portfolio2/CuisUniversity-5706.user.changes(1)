!Trailer methodsFor: 'accessing' stamp: 'AS 4/30/2023 14:19:45' prior: 50642551!
totalProductsHeight

	^(container select: [ :product | product isStackable])
		sum: [ :product | product height ]
		ifEmpty: [0]

! !

