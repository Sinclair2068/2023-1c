!City methodsFor: 'as yet unclassified' stamp: 'AS 4/26/2023 23:40:08' prior: 50619203!
hasResidentialZones

	| index |
	zones ifNil: [ ^ false ].

	index _ 1.
	
	[index <= zones size] whileTrue: [ | zone |
		zone _ zones at: index.
		zone type = #ResidentialZone ifTrue: [ ^ true].	
		index := index + 1.
	].

	^ false.
! !
