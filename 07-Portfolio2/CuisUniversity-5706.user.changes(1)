		with: (self createSectorWithTurboOf: 10 * kilometer)
		with: (self createSectorWithoutTurboOf: 20 * kilometer).
	
	grandPrix := GrandPrix 
		on: (Track withSectors: sectors)
		running: 100
		with: (Set with: verstappen).
		
