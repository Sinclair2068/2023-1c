test17NorthThreeLeftOneForwardCommandsPointsEastMovesOneEast

	| marsRover |

	marsRover := MarsRover startOn: 1@1 facing: self north.
	
	marsRover giveCommands: 'lllf'.

	self assertRover: marsRover at: 2@1 pointing: self south.	
	
	! !

