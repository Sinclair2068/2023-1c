!methodRemoval: Portfolio #accounts stamp: 'WAL 5/20/2023 16:22:26'!
accounts

	| allAccounts |

	allAccounts := OrderedCollection new.
	allAccounts addAll: accounts.
	
	portfolios do: [ : portfolio | allAccounts addAll: portfolio accounts ].

	^allAccounts!

