test10TransferNetOnPortfolioWithTransferShouldReturnTransferLines

	| account1 account2 report portfolio |
	
	account1 := ReceptiveAccount  new.
	account2 := ReceptiveAccount  new.

	Transfer register: 100 from: account1 to: account2.
	Transfer register: 70 from: account2 to: account1.
	
	portfolio := Portfolio with: account1.
	
	report := TransferNet on: portfolio.
	
	self assert: 'Neto de transferencias = -30' equals: (report lines at: 1).

