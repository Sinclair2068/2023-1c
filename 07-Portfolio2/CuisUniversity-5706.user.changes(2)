!classDefinition: #StructuredAccountSummary category: 'Portfolio-Solucion' stamp: 'WAL 5/27/2023 20:05:02'!
Report subclass: #StructuredAccountSummary
	instanceVariableNames: 'indentation'
	classVariableNames: ''
	poolDictionaries: ''
	category: 'Portfolio-Solucion'!
!StructuredAccountSummary methodsFor: 'write' stamp: 'WAL 5/27/2023 20:04:58'!
writePortfolio: aPortfolio

	lines add: (String new: indentation withAll: $ ), aPortfolio name.
	
	indentation := indentation + 3.
	
	aPortfolio writeTo: self.
	
	lines add: 'Balance = ', aPortfolio balance asString.	
	
	indentation := indentation - 3.
	
! !

!methodRemoval: PortfolioStructure #writeName: stamp: 'WAL 5/27/2023 20:08:14'!
writeName: aName

	lines add: aName!
