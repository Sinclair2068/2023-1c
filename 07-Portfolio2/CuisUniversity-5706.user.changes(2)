transactions
	
	| transactions |
	
	transactions := OrderedCollection new.
	
	"transactions addAll: (elements do: [ :element | ^element transactions ])."
	
	self addTransactionsTo: transactions.
	
	elements do: [ :element | element addTransactionsTo: transactions ].
		
	^transactions.
		! !

