test15NorthOneForwardOneLeftCommandsMovesOneForwardPointsWest

	| marsRover |

	marsRover := MarsRover startOn: 1@1 facing: self north.
	
	marsRover giveCommands: 'lf'.

	self assertRover: marsRover at: 1@2 pointing: self west.	
	
	! !

