test11PortfolioStructureOnPortfolioShouldReturnPortfolioStructureReport

	| portfolio portfolioStructure |
	
	portfolio := self portfolioDeLaFamilia
		with: (self cuentaMia)
		with: ( self portfolioDeHijos
			with: (	self cuentaDeJuan)
			with: (self cuentaDeAngeles)).
	
	portfolioStructure := PortfolioStructure on: portfolio.
	
	self assert: portfolioStructure sameAs: {
		self portfolioDeLaFamilia name.
		self line: self cuentaMia name withMargin: 3.
		}.
	
	self assert: 'Portfolio de la familia' equals: (portfolioStructure lines at: 1).
	self assert: '   Cuenta Mia' equals: (portfolioStructure lines at: 2).
	self assert: '   Portfolio de hijos' equals: (portfolioStructure lines at: 3).
	self assert: '      Cuenta de Juan' equals: (portfolioStructure lines at: 4).
	self assert: '      Cuenta de Angeles' equals: (portfolioStructure lines at: 5).
! !
!ReportTest methodsFor: 'lines' stamp: 'WAL 5/28/2023 17:44:15'!
line: aText withTabs: aSize

	^ String new: aSize withAll: self tabCharacter , aText! !
!ReportTest methodsFor: 'as yet unclassified' stamp: 'WAL 5/28/2023 17:44:15' prior: 50630341!
test11PortfolioStructureOnPortfolioShouldReturnPortfolioStructureReport

	| portfolio portfolioStructure |
	
	portfolio := self portfolioDeLaFamilia
		with: (self cuentaMia)
		with: ( self portfolioDeHijos
			with: (	self cuentaDeJuan)
			with: (self cuentaDeAngeles)).
	
	portfolioStructure := PortfolioStructure on: portfolio.
	
	self assert: portfolioStructure sameAs: {
		self portfolioDeLaFamilia name.
		self line: self cuentaMia name withTabs: 3.
		}.
	
	self assert: 'Portfolio de la familia' equals: (portfolioStructure lines at: 1).
	self assert: '   Cuenta Mia' equals: (portfolioStructure lines at: 2).
	self assert: '   Portfolio de hijos' equals: (portfolioStructure lines at: 3).
	self assert: '      Cuenta de Juan' equals: (portfolioStructure lines at: 4).
	self assert: '      Cuenta de Angeles' equals: (portfolioStructure lines at: 5).
! !

!methodRemoval: ReportTest #line:withMargin: stamp: 'WAL 5/28/2023 17:44:15'!
line: aText withMargin: aSize

	^ String new: aSize withAll: self tabCharacter , aText!
!ReportTest methodsFor: 'lines' stamp: 'WAL 5/28/2023 17:45:07' prior: 50630370!
line: aText withTabs: aSize

	^ String new: aSize * self tabSize withAll: self tabCharacter , aText! !
