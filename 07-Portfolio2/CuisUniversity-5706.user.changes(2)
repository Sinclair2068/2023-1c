!StructuredReport methodsFor: 'as yet unclassified' stamp: 'WAL 5/27/2023 20:52:17' prior: 50626082 overrides: 50625858!
initialize

	super initialize.

	indentationSize := 0.
	tabSize := 3.
	tabCharacter := Character space.
! !
!StructuredAccountSummary methodsFor: 'indentation' stamp: 'WAL 5/27/2023 20:52:52' prior: 50625736!
indentation

	^ String new: indentationSize withAll: tabCharacter.! !
!StructuredAccountSummary methodsFor: 'indentation' stamp: 'WAL 5/27/2023 20:53:14' prior: 50625688!
decrementIndentation

	^ indentationSize := indentationSize - tabSize! !
!StructuredAccountSummary methodsFor: 'indentation' stamp: 'WAL 5/27/2023 20:53:20' prior: 50625652!
incrementIndentation

	^ indentationSize := indentationSize + tabSize! !
!StructuredReport methodsFor: 'indentation' stamp: 'WAL 5/27/2023 20:53:44'!
indentation

	^ String new: indentationSize withAll: tabCharacter.! !

!methodRemoval: StructuredAccountSummary #indentation stamp: 'WAL 5/27/2023 20:53:44'!
indentation

	^ String new: indentationSize withAll: tabCharacter.!
!StructuredReport methodsFor: 'indentation' stamp: 'WAL 5/27/2023 20:53:47'!
incrementIndentation

	^ indentationSize := indentationSize + tabSize! !

!methodRemoval: StructuredAccountSummary #incrementIndentation stamp: 'WAL 5/27/2023 20:53:47'!
incrementIndentation

	^ indentationSize := indentationSize + tabSize!
!StructuredReport methodsFor: 'indentation' stamp: 'WAL 5/27/2023 20:53:51'!
decrementIndentation

	^ indentationSize := indentationSize - tabSize! !

!methodRemoval: StructuredAccountSummary #decrementIndentation stamp: 'WAL 5/27/2023 20:53:51'!
decrementIndentation

	^ indentationSize := indentationSize - tabSize!
!StructuredReport methodsFor: 'write' stamp: 'WAL 5/27/2023 20:55:01'!
writeLine: aText

	lines add: (self indentation), aText! !

!methodRemoval: StructuredAccountSummary #writeLine: stamp: 'WAL 5/27/2023 20:55:01'!
writeLine: aText

	lines add: (self indentation), aText!
