	self assert: '      Cuenta de Juan' equals: (report lines at: 7).
	self assert: '         Salida por transferencia de 200' equals: (report lines at: 8).	
	self assert: '         Balance = -200' equals: (report lines at: 9).	
	self assert: '      Cuenta de Angeles' equals: (report lines at: 10).
	self assert: '         Entrada por transferencia de 200' equals: (report lines at: 11).	
	self assert: '         Balance = 200' equals: (report lines at: 12).	
	self assert: '      Balance = 0' equals: (report lines at: 13).	
	self assert: '   Balance = 30' equals: (report lines at: 14).	
	! !
